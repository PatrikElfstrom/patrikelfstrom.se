{
  "name": "patrikelfstrom.se",
  "version": "1.0.0",
  "description": "Patrik Elfström portfolio",
  "main": "patrikelfstrom.js",
  "scripts": {
    "clean-dist": "rm -rf dist",
    "clean-root": "rm -f dist",
    "clean-scripts": "rm -rf dist/scripts",
    "clean-styles": "rm -rf dist/styles",
    "clean-images": "rm -rf dist/images",
    "copy": "copy 'app/**/*.*' '!app/scripts/**/*.*' '!app/images/**/*.*' '!app/styles/**/*.*' dist",
    "build-images": "node build/build-images.js",
    "build-sw": "node build/build-sw.js",
    "build-scripts": "node build/build-scripts.js",
    "build-styles": "node build/build-styles.js",
    "build-html": "node build/build-html.js",
    "build": "node build/build.js",
    "build-production": "NODE_ENV=production node build/build.js",
    "watch-scripts": "node build/watch-scripts.js",
    "watch-styles": "node build/watch-styles.js",
    "watch-images": "node build/watch-images.js",
    "watch-root": "node build/watch-root.js",
    "watch": "node build/watch.js",
    "deploy": "npm run deploy-develop",
    "deploy-develop": "npm run build; firebase deploy -P develop",
    "deploy-production": "npm run build-production; firebase deploy -P production"
  },
  "author": "Patrik Elfström <me@patrikelfstrom.se> (https://patrikelfstrom.se)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "spdy": "^3.4.7",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "clean-css": "^4.1.11",
    "copy": "^0.3.2",
    "glob": "^7.1.2",
    "handlebars": "^4.0.11",
    "html-minifier": "^3.5.16",
    "imagemin": "^5.3.1",
    "imagemin-jpegoptim": "^5.2.0",
    "imagemin-optipng": "^5.2.1",
    "imagemin-svgo": "^6.0.0",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.23",
    "resolve": "^1.8.1",
    "rollup": "^0.56.5",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "shelljs": "^0.8.2",
    "shrink-ray": "^0.1.3",
    "uglify-es": "^3.3.10",
    "watch": "^1.0.2",
