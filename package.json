  "scripts": {
    "build-production": "NODE_ENV=production npx webpack --mode production --display-error-details  --config webpack.config.js",
    "build": "npx webpack --mode development --display-error-details  --config webpack.config.js",
    "serve": "npx http-server ./dist",
    "test": "jest --watch",
    "dev": "npx webpack-dev-server --mode development --display-error-details --config webpack.config.js --debug --output-pathinfo",
    "deploy": "npm run deploy-develop",
    "deploy-develop": "npm run build-production; npx firebase deploy -P develop",
    "deploy-production": "npm run build-production; npx firebase deploy -P production"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
